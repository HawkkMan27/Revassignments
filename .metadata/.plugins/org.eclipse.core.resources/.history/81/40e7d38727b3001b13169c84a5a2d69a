
public class ConditionalBlocks {
	public static void main(String[] args) {  
		   boolean b = true;  
		 
	if (b){  
		  System.out.println("inside the if-statement");  
		   }  
	else {
	      System.out.println("inside the else-statement");
	    }
		   System.out.println("Outside of the if-statement");  
		 }
	}

